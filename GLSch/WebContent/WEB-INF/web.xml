<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
 
<!--  Scheduler -->
  <display-name>GLSch</display-name>
  <servlet>
    <description>JAX-RS Tools Generated - Do not modify</description>
    <servlet-name>GLSch JAX-RS Servlet</servlet-name>
    <servlet-class>org.apache.cxf.transport.servlet.CXFServlet</servlet-class>
    <init-param>
    	<param-name>config-location</param-name>
		<param-value>/WEB-INF/beanSch.xml</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>GLSch JAX-RS Servlet</servlet-name>
    <url-pattern>/Sch/*</url-pattern>
  </servlet-mapping>


<!--   SchedulerResponse -->
<!--   <display-name>GLSchResp</display-name> -->
  <servlet>
    <description>JAX-RS Tools Generated - Do not modify</description>
    <servlet-name>GLSchResp JAX-RS Servlet</servlet-name>
    <servlet-class>org.apache.cxf.transport.servlet.CXFServlet</servlet-class>
    <init-param>
    	<param-name>config-location</param-name>
		<param-value>/WEB-INF/beanSchResp.xml</param-value>
    </init-param>
    <load-on-startup>2</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>GLSchResp JAX-RS Servlet</servlet-name>
    <url-pattern>/SchResp/*</url-pattern>
  </servlet-mapping>

</web-app>